<?xml version="1.0"?>
<odoo>
    <record id="bhs_view_department_form_inherit" model="ir.ui.view">
			<field name="name">bhs.hr.department.form.inherit</field>
			<field name="model">hr.department</field>
			<field name="inherit_id" ref="hr.view_department_form"/>
